{
  "name": "ds-json",
  "displayName": "ds-json",
  "description": "Language support for JSON-based DataScript.",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/mrpedrobraga/godot-datascript-3.x.git"
  },
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ds-json",
        "aliases": [
          "Datascript (JSON)",
          "ds-json"
        ],
        "extensions": [
          ".ds.json"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ds-json",
        "scopeName": "source.ds-json",
        "path": "./syntaxes/ds-json.tmLanguage.json"
      }
    ]
  }
}
